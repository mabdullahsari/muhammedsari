{
    "name": "mabdullahsari/muhammedsari",
    "type": "project",
    "description": "The source of muhammedsari.me",
    "license": "MIT",
    "require": {
        "php": "~8.1",
        "dive-be/eloquent-utils": "^0.1.1",
        "dive-be/php-enum-utils": "^1.1",
        "filament/filament": "^2.16.34",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.36.1",
        "laravel/tinker": "^2.7.2",
        "shuvroroy/filament-spatie-laravel-health": "^1.6",
        "spatie/filament-markdown-editor": "^1.0",
        "spatie/laravel-health": "^1.15.1",
        "spatie/security-advisories-health-check": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.1",
        "nunomaduro/larastan": "^2.2.5",
        "phpunit/phpunit": "^9.5.25",
        "qossmic/deptrac-shim": "^0.23.0",
        "spatie/laravel-ignition": "^1.5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "deptrac": "@php ./vendor/bin/deptrac",
        "format": "@php ./vendor/bin/pint",
        "larastan": "@php ./vendor/bin/phpstan analyse --memory-limit=2G",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade"
        ],
        "test": "@php artisan test",
        "verify": [
            "@deptrac",
            "@larastan",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "akaunting/laravel-money",
                "blade-ui-kit/blade-heroicons",
                "blade-ui-kit/blade-icons",
                "filament/filament",
                "filament/forms",
                "filament/notifications",
                "filament/support",
                "filament/tables",
                "intervention/image",
                "laravel/tinker",
                "livewire/livewire",
                "nesbot/carbon",
                "nunomaduro/termwind",
                "ryangjchandler/blade-capture-directive",
                "shuvroroy/filament-spatie-laravel-health",
                "spatie/filament-markdown-editor",
                "spatie/laravel-health"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
