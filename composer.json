{
    "name": "mabdullahsari/muhammedsari",
    "type": "project",
    "description": "The source of muhammedsari.me",
    "license": "MIT",
    "require": {
        "php": "~8.2",
        "abraham/twitteroauth": "^6.1",
        "blade-ui-kit/blade-icons": "^1.5.2",
        "dive-be/php-enum-utils": "^1.2",
        "filament/filament": "^3.0.52",
        "guzzlehttp/guzzle": "^7.8.0",
        "laravel/framework": "^10.23.1",
        "laravel/horizon": "^5.20.1",
        "laravel/tinker": "^2.8.2",
        "psr/clock": "^1.0",
        "shuvroroy/filament-spatie-laravel-health": "^2.0",
        "spatie/browsershot": "^3.58.2",
        "spatie/laravel-feed": "^4.3.0",
        "spatie/laravel-health": "^1.23.4",
        "spatie/laravel-honeypot": "^4.3.4",
        "spatie/laravel-markdown": "^2.4.0",
        "spatie/security-advisories-health-check": "^1.1.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13.1",
        "mockery/mockery": "^1.6.6",
        "nunomaduro/collision": "^7.8.1",
        "nunomaduro/larastan": "^2.6.4",
        "phpunit/phpunit": "^10.3.4",
        "spatie/laravel-ignition": "^2.3.0",
        "spatie/laravel-ray": "^1.33.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Blogging\\": "src/Blogging",
            "Clock\\": "src/Clock",
            "CommandBus\\": "src/CommandBus",
            "Contacting\\": "src/Contacting",
            "Database\\": "src/Database",
            "Database\\Seeders\\": "database/seeders",
            "Identity\\": "src/Identity",
            "Monitoring\\": "src/Monitoring",
            "Notifying\\": "src/Notifying",
            "PreservingData\\": "src/PreservingData",
            "PreventingSpam\\": "src/PreventingSpam",
            "Previewing\\": "src/Previewing",
            "Publishing\\": "src/Publishing",
            "Queue\\": "src/Queue",
            "Scheduling\\": "src/Scheduling",
            "SharedKernel\\": "src/SharedKernel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "@php ./vendor/bin/pint",
        "larastan": "@php ./vendor/bin/phpstan analyse --memory-limit=2G",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "test": "@php ./vendor/bin/phpunit",
        "verify": [
            "@larastan",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "akaunting/laravel-money",
                "blade-ui-kit/blade-heroicons",
                "blade-ui-kit/blade-icons",
                "filament/actions",
                "filament/filament",
                "filament/forms",
                "filament/infolists",
                "filament/notifications",
                "filament/support",
                "filament/tables",
                "filament/widgets",
                "intervention/image",
                "kirschbaum-development/eloquent-power-joins",
                "laravel/horizon",
                "laravel/tinker",
                "livewire/livewire",
                "nesbot/carbon",
                "nunomaduro/termwind",
                "ryangjchandler/blade-capture-directive",
                "shuvroroy/filament-spatie-laravel-health",
                "spatie/filament-markdown-editor",
                "spatie/laravel-feed",
                "spatie/laravel-health",
                "spatie/laravel-honeypot",
                "spatie/laravel-markdown",
                "spatie/laravel-route-attributes"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
