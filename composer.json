{
    "name": "mabdullahsari/muhammedsari",
    "type": "project",
    "description": "The source of muhammedsari.me",
    "license": "MIT",
    "require": {
        "php": "~8.2",
        "abraham/twitteroauth": "^5.0",
        "blade-ui-kit/blade-icons": "^1.5",
        "dive-be/php-enum-utils": "^1.2",
        "filament/filament": "^2.17.30",
        "gajus/dindent": "^2.0.2",
        "guzzlehttp/guzzle": "^7.5.1",
        "laravel/framework": "^10.9",
        "laravel/tinker": "^2.8.1",
        "shuvroroy/filament-spatie-laravel-health": "^1.6",
        "spatie/filament-markdown-editor": "^1.2.2",
        "spatie/laravel-feed": "^4.2.1",
        "spatie/laravel-health": "^1.22.5",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-markdown": "^2.2.7",
        "spatie/security-advisories-health-check": "^1.0.1",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/pint": "1.7",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.5.2",
        "nunomaduro/larastan": "^2.6.0",
        "phpunit/phpunit": "^10.1.2",
        "spatie/laravel-ignition": "^2.1",
        "spatie/laravel-ray": "^1.32.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Blogging\\": "src/Blogging",
            "Clock\\": "src/Clock",
            "Contacting\\": "src/Contacting",
            "Database\\": "database/",
            "Database\\Seeders\\": "database/seeders",
            "HtmlBeautifier\\": "src/HtmlBeautifier",
            "Identity\\": "src/Identity",
            "Monitoring\\": "src/Monitoring",
            "Publishing\\": "src/Publishing",
            "Scheduling\\": "src/Scheduling"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "@php ./vendor/bin/pint",
        "larastan": "@php ./vendor/bin/phpstan analyse --memory-limit=2G",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade"
        ],
        "test": "@php ./vendor/bin/phpunit",
        "verify": [
            "@larastan",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "akaunting/laravel-money",
                "blade-ui-kit/blade-heroicons",
                "blade-ui-kit/blade-icons",
                "filament/filament",
                "filament/forms",
                "filament/notifications",
                "filament/support",
                "filament/tables",
                "intervention/image",
                "laravel/tinker",
                "livewire/livewire",
                "nesbot/carbon",
                "nunomaduro/termwind",
                "ryangjchandler/blade-capture-directive",
                "shuvroroy/filament-spatie-laravel-health",
                "spatie/filament-markdown-editor",
                "spatie/laravel-feed",
                "spatie/laravel-health",
                "spatie/laravel-markdown",
                "spatie/laravel-route-attributes"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
