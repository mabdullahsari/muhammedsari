{
    "name": "mabdullahsari/muhammedsari",
    "type": "project",
    "description": "The source of muhammedsari.me",
    "license": "MIT",
    "require": {
        "php": "~8.1",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.35.1",
        "laravel/tinker": "^2.7.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.1",
        "nunomaduro/larastan": "^2.2.4",
        "phpunit/phpunit": "^9.5.25",
        "spatie/laravel-ignition": "^1.5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "larastan": "@php ./vendor/bin/phpstan analyse --memory-limit=2G",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": "@php artisan test",
        "verify": [
            "@larastan",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
